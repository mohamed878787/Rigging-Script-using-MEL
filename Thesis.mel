SN_lAnkle.sx $rAnkleScale[0];		setAttr  DSN_lAnkle.sy $rAnkleScale[1];		setAttr  DSN_lAnkle.sz $rAnkleScale[2];				$rBallTranslate = `xform -q -t DSN_rBall`;		$rBallRotate = `xform -q -ro DSN_rBall`;		$rBallScale = `xform -q -r -s DSN_rBall`;		setAttr  DSN_lBall.tx ($rBallTranslate[0]*-1);		setAttr  DSN_lBall.tz $rBallTranslate[2];		setAttr  DSN_lBall.ry ($rBallRotate[1]*-1);		setAttr  DSN_lBall.sx $rBallScale[0];		setAttr  DSN_lBall.sy $rBallScale[1];		setAttr  DSN_lBall.sz $rBallScale[2];				$rToeTranslate = `xform -q -t DSN_rToe`;		$rToeRotate = `xform -q -ro DSN_rToe`;		$rToeScale = `xform -q -r -s DSN_rToe`;		setAttr  DSN_lToe.tx ($rToeTranslate[0]*-1);		setAttr  DSN_lToe.tz $rToeTranslate[2];		setAttr  DSN_lToe.ry ($rToeRotate[1]*-1);		setAttr  DSN_lToe.sx $rToeScale[0];		setAttr  DSN_lToe.sy $rToeScale[1];		setAttr  DSN_lToe.sz $rToeScale[2];			}	else	{		error "NO DUMMY RIG TO MIRROR!\n";	}}global proc basicReset(){	if (`objExists  DSN_MAIN`) 	{		string $resets[] = `ls DSN_ROOT DSN_MainHip DSN_lHip DSN_rHip DSN_Spine01 DSN_Spine02		DSN_Spine03 DSN_Spine04 DSN_Neck DSN_Head DSN_Jaw DSN_JawTip DSN_HeadTip		DSN_lEye DSN_rEye DSN_lClavicle DSN_lShoulder DSN_rClavicle DSN_rShoulder		DSN_lElbow DSN_lWrist DSN_lThumb1 DSN_lThumb2 DSN_lThumb3 DSN_lThumbTip		DSN_lIndex1 DSN_lIndex2 DSN_lIndex3 DSN_lIndexTip DSN_lMiddle1 DSN_lMiddle2		DSN_lMiddle3 DSN_lMiddleTip DSN_lRing1 DSN_lRing2 DSN_lRing3 DSN_lRingTip		DSN_lPinkie1 DSN_lPinkie2 DSN_lPinkie3 DSN_lPinkieTip DSN_lKnee DSN_lAnkle		DSN_rElbow DSN_rWrist DSN_rThumb1 DSN_rThumb2 DSN_rThumb3 DSN_rThumbTip		DSN_rIndex1 DSN_rIndex2 DSN_rIndex3 DSN_rIndexTip DSN_rMiddle1 DSN_rMiddle2		DSN_rMiddle3 DSN_rMiddleTip DSN_rRing1 DSN_rRing2 DSN_rRing3 DSN_rRingTip		DSN_rPinkie1 DSN_rPinkie2 DSN_rPinkie3 DSN_rPinkieTip DSN_rKnee DSN_rAnkle		DSN_lBall DSN_lToe DSN_rBall DSN_rToe`;		for ($each in $resets)		{			string $attr;			for ($attr in `listAttr -k -u $each`)			{				if (`gmatch $attr "scale*"` || `gmatch $attr "visibility"`)				{					setAttr ($each + "." + $attr) 1;				}				else				{					setAttr ($each + "." + $attr) 0;				}			}		}		print "SUCCESS! CHARACTER RESET TO DEFAULT PROPORTIONS\n";	}	else	{		error "NO DUMMY RIG TO RESET!\n";	}}global proc generateBasicRig(){	if (`objExists  DSN_MAIN`)	{		print "BUILDING RIG...\n";					//TURN OFF REFLECTION!		symmetricModelling -e -symmetry 0;				//DELETE BUGGY CONSTRAINT		delete DSN_Spine01G_pointConstraint1 DSN_Spine02G_pointConstraint1		DSN_Spine03G_pointConstraint1;				//UNLOCK MAIN CONTROL		setAttr -l false -k true -channelBox false "DSN_MAIN.translate";		setAttr -l false -k true -channelBox false "DSN_MAIN.rotate";				////////////////CREATE JOINTS////////////////////////////////////////////				select -cl;		joint -n ROOTJ;		delete `pointConstraint DSN_ROOT ROOTJ`;				select ROOTJ;		joint -n Spine01J;		delete `pointConstraint DSN_Spine01 Spine01J`;				select ROOTJ;		joint -n MainHipJ;		delete `pointConstraint DSN_MainHip MainHipJ`;				select Spine01J;		joint -n Spine02J;		delete `pointConstraint DSN_Spine02 Spine02J`;				select Spine02J;		joint -n Spine03J;		delete `pointConstraint DSN_Spine03 Spine03J`;				select Spine03J;		joint -n Spine04J;		delete `pointConstraint DSN_Spine04 Spine04J`;				select Spine04J;		joint -n NeckJ;		delete `pointConstraint DSN_Neck NeckJ`;				select NeckJ;		joint -n HeadJ;		delete `parentConstraint DSN_Head HeadJ`; 				select HeadJ;		joint -n HeadTipJ;		delete `pointConstraint DSN_HeadTip HeadTipJ`;				select HeadJ;		joint -n JawJ;		delete `pointConstraint DSN_Jaw JawJ`;				select JawJ;		joint -n JawTipJ;		delete `pointConstraint DSN_JawTip JawTipJ`;				select HeadJ;		joint -n lEyeJ;		delete `pointConstraint DSN_lEye lEyeJ`;				select HeadJ;		joint -n rEyeJ;		delete `pointConstraint DSN_rEye rEyeJ`;						//////////////////////LEFT ARM//////////////////////		select Spine04J;		joint -n lClavicleJ;		delete `pointConstraint DSN_lClavicle lClavicleJ`;				select lClavicleJ;		joint -n lShoulderJ;		delete `pointConstraint DSN_lShoulder lShoulderJ`;				select lShoulderJ;		joint -n lElbowJ;		delete `pointConstraint DSN_lElbow lElbowJ`;				select lElbowJ;		joint -n lWristJ;		delete `pointConstraint DSN_lWrist lWristJ`;		$lWristJRotation = `xform -q -ro DSN_lWrist`;				setAttr "lWristJ.jointOrientX" $lWristJRotation[0];		setAttr "lWristJ.jointOrientY" $lWristJRotation[1];		setAttr "lWristJ.jointOrientZ" $lWristJRotation[2];				//LEFT THUMB		select -cl;		joint -n lThumb1J;		delete `pointConstraint DSN_lThumb1 lThumb1J`;				select lThumb1J;		joint -n lThumb2J;		delete `pointConstraint DSN_lThumb2 lThumb2J`;				select lThumb2J;		joint -n lThumb3J;		delete `pointConstraint DSN_lThumb3 lThumb3J`;				select lThumb3J;		joint -n lThumbTipJ;		delete `pointConstraint DSN_lThumbTip lThumbTipJ`;				//LEFT INDEX		select -cl;		joint -n lIndex1J;		delete `pointConstraint DSN_lIndex1 lIndex1J`;				select lIndex1J;		joint -n lIndex2J;		select DSN_lIndex2 lIndex2J;		delete `pointConstraint DSN_lIndex2 lIndex2J`; 				select lIndex2J;		joint -n lIndex3J;		delete `pointConstraint DSN_lIndex3 lIndex3J`;				select lIndex3J;		joint -n lIndexTipJ;		delete `pointConstraint DSN_lIndexTip lIndexTipJ`;				//LEFT MIDDLE		select -cl;		joint -n lMiddle1J;		delete `pointConstraint DSN_lMiddle1 lMiddle1J`; 				select lMiddle1J;		joint -n lMiddle2J;		delete `pointConstraint DSN_lMiddle2 lMiddle2J`;				select lMiddle2J;		joint -n lMiddle3J;		delete `pointConstraint DSN_lMiddle3 lMiddle3J`;				select lMiddle3J;		joint -n lMiddleTipJ;		delete `pointConstraint DSN_lMiddleTip lMiddleTipJ `;				//LEFT RING		select -cl;		joint -n lRing1J;		delete `pointConstraint DSN_lRing1 lRing1J`;				select lRing1J;		joint -n lRing2J;		delete `pointConstraint DSN_lRing2 lRing2J`;				select lRing2J;		joint -n lRing3J;		delete `pointConstraint DSN_lRing3 lRing3J`;				select lRing3J;		joint -n lRingTipJ;		delete `pointConstraint DSN_lRingTip lRingTipJ`;				//LEFT PINKIE		select -cl;		joint -n lPinkie1J;		delete `pointConstraint DSN_lPinkie1 lPinkie1J`;				select lPinkie1J;		joint -n lPinkie2J;		delete `pointConstraint DSN_lPinkie2 lPinkie2J`;				select lPinkie2J;		joint -n lPinkie3J;		delete `pointConstraint DSN_lPinkie3 lPinkie3J`;				select lPinkie3J;		joint -n lPinkieTipJ;		delete `pointConstraint DSN_lPinkieTip lPinkieTipJ`;				//PARENT FINGERS TO WRIST		parent lThumb1J lIndex1J lMiddle1J lRing1J lPinkie1J lWristJ;				//LEFT FOREARM JOINT		select -cl;		joint -n lForearmJ;		delete `pointConstraint lWristJ lElbowJ lForearmJ`;		parent lForearmJ lElbowJ;		joint -e -oj none -secondaryAxisOrient yup -zso;				//////////////////////RIGHT ARM//////////////////////		select Spine04J;		joint -n rClavicleJ;		delete `pointConstraint DSN_rClavicle rClavicleJ`;				select rClavicleJ;		joint -n rShoulderJ;		delete `pointConstraint DSN_rShoulder rShoulderJ`;				select rShoulderJ;		joint -n rElbowJ;		delete `pointConstraint DSN_rElbow rElbowJ`;				select rElbowJ;		joint -n rWristJ;		delete `pointConstraint DSN_rWrist rWristJ`;		$rWristJRotation = `xform -q -ro DSN_rWrist`;		setAttr "rWristJ.jointOrientX" $rWristJRotation[0];		setAttr "rWristJ.jointOrientY" $rWristJRotation[1];		setAttr "rWristJ.jointOrientZ" $rWristJRotation[2];						//LEFT THUMB		select -cl;		joint -n rThumb1J;		delete `pointConstraint DSN_rThumb1 rThumb1J`;				select rThumb1J;		joint -n rThumb2J;		delete `pointConstraint DSN_rThumb2 rThumb2J`;				select rThumb2J;		joint -n rThumb3J;		delete `pointConstraint DSN_rThumb3 rThumb3J`;				select rThumb3J;		joint -n rThumbTipJ;		delete `pointConstraint DSN_rThumbTip rThumbTipJ`;				//LEFT INDEX		select -cl;		joint -n rIndex1J;		delete `pointConstraint DSN_rIndex1 rIndex1J`;				select rIndex1J;		joint -n rIndex2