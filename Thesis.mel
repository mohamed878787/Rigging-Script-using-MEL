global proc Thesis(){	if (`window -exists DSN_BiPedBasic`) 	deleteUI DSN_BiPedBasic; 		window -title "Bachelor Thesis"		-wh 150 150		-rtf on		-mnb false		-mxb false		-s false		DSN_BiPedBasic;			columnLayout;	//STEP 1		frameLayout -label "Step 1: Create template"			-width 310;						button -label "Create template"			-command "createBasicProxies"			-bgc 0.6 0.6 1			createBasicProxies;				setParent ..;  //Back up to the columLayout		setParent ..;  //Back up to the columLayout			//STEP 2		frameLayout -label "Step 2: Mirror template"			-width 310;						button -label "Mirror Left side to Right side"			-command "basicLeftToRight"			basicLeftToRight;						setParent ..;  //Back up to the columLayout		setParent ..;  //Back up to the columLayout		//STEP 3		frameLayout -label "Step 3: Create Rig"			-width 310;			button -label "create Rig"			-command "generateBasicRig"			-bgc 0.2 0.6 0.2			generateBasicRig;				setParent ..;  //Back up to the columLayout		setParent ..;  //Back up to the columLayout							showWindow DSN_BiPedBasic;}global proc createBasicProxies(){	if (`objExists  DSN_MAIN`) 	{		error "RIG ALREADY IN PROGRESS! NO ACTION TAKEN.\n";	}	else if (`objExists  MainC`)	{		error "A RIG ALREADY EXISTS IN THIS SCENE. NO ACTION TAKEN.\n";	}	else	{		//CREATE JOINT BASE		circle -n DSN_MAIN -c 0 0 0 -nr 0 1 0 -sw 360 -r 8 -d 3 -ut 0 -tol 0.01 -s 8 -ch 0;		curve -n DSN_MAIN_ARROW -d 1 -p 3 0 7.406856 -p 3 0 8 -p 5 0 8 -p 0 0 11 -p -5 0 8 -p -3 0 8 -p -3 0 7.406856 -k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 ;		pickWalk -d down;		select -add DSN_MAIN;		parent -r -s;		delete DSN_MAIN_ARROW;				circle -n DSN_ROOT -c 0 0 0 -nr 0 1 0 -sw 360 -r 0.25 -d 3 -ut 0 -tol 0.01 -s 8 -ch 0;		duplicate -rr -n DSN_ROOTB;		rotate 90 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_ROOTC;		rotate 0 90 0 ;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		parent -r -s DSN_ROOTBShape DSN_ROOTCShape DSN_ROOT;		delete DSN_ROOTB DSN_ROOTC;		select DSN_ROOT;		move -r -os -wd 0 15.25 0;		scale -r 1.6 1.6 1.6;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;				//SPINE JOINTS		duplicate -rr -n DSN_Spine01;		group -n DSN_Spine01G;		CenterPivot;		scale -r 0.625 0.625 0.625;		move -r -os -wd 0 1.65 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_Spine02G;		pickWalk -d down;		rename "DSN_Spine02";		pickWalk -d up;		move -r -os -wd 0 1.65 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_Spine03G;		pickWalk -d down;		rename "DSN_Spine03";		pickWalk -d up;		move -r -os -wd 0 1.65 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_Spine04G;		pickWalk -d down;		rename "DSN_Spine04";		pickWalk -d up;		ungroup;		scale -r 1.6 1.6 1.6;		move -r -os -wd 0 1.65 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_Neck;		scale -r 0.625 0.625 0.625;		move -r -os -wd 0 1.65 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;				//NECK AND HEAD JOINTS		duplicate -rr -n DSN_Head;		move -r -os -wd 0 0.9 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_Jaw;		move -r -os -wd 0 0.83 0.38;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_JawTip;		move -r -os -wd 0 -0.83 1.52;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		select DSN_Head;		duplicate -rr -n DSN_HeadTip;		move -r -os -wd 0 3.38 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;				//LEFT EYE		duplicate -rr -n DSN_lEye DSN_Head;		move -r -os -wd 0.57 1.53 1.64 DSN_lEye;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0 DSN_lEye;				//RIGHT EYE		duplicate -rr -n DSN_rEye DSN_Head;		move -r -os -wd -0.57 1.53 1.64 DSN_rEye;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0 DSN_rEye;						//LEFT ARM//////////////////////////////////		select DSN_Spine04;		duplicate -rr -n DSN_lClavicle;		scale -r 0.625 0.625 0.625;		move -r -os -wd 1.25 0.5 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_lShoulder;		move -r -os -wd 1.7 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_lElbow;		move -r -os -wd 3.5 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		curve -n DSN_lElbowAim -d 1 -p -0.25 0 0 -p -0.25 0 -1 -p -0.5 0 -1 -p 0 0 -1.5 -p 0.5 0 -1 -p 0.25 0 -1 -p 0.25 0 0-k 0 -k 1 -k 2 -k 3 -k 4 -k 5 -k 6 ;		select DSN_lElbow DSN_lElbowAim;		pointConstraint;		delete DSN_lElbowAim_pointConstraint1;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		select DSN_lElbowAim;		pickWalk -d down;		select -add DSN_lElbow;		parent -r -s;		delete DSN_lElbowAim;								//LEFT HAND				//LEFT WRIST		circle -n DSN_lWrist -nr 0 1 0 -sw 360 -r 1 -d 3 -ut 0 -tol 0.01 -s 8 -ch 0;		circle -n DSN_lWrist2 -nr 1 0 0 -sw 360 -r 1 -d 3 -ut 0 -tol 0.01 -s 8 -ch 0;		circle -n DSN_lWrist3 -nr 0 0 1 -sw 360 -r 1 -d 3 -ut 0 -tol 0.01 -s 8 -ch 0;		curve -n DSN_lWrist4 -d 1 -p 0 -1 0 -p 0 3 0 -k 0 -k 1 ;		curve -n DSN_lWrist5 -d 1 -p 0 2.5 0.5 -p 0 3 0 -p 0 2.5 -0.5 -k 0 -k 1 -k 2 ;		curve -n DSN_lWrist6 -d 1 -p -0.5 2.5 0 -p 0 3 0 -p 0.5 2.5 0 -k 0 -k 1 -k 2 ;		circle -n DSN_lWrist7 -c 0 2.5 0 -nr 0 1 0 -sw 360 -r 0.5 -d 3 -ut 0 -tol 0.01 -s 8 -ch 0;				select DSN_lWrist2 DSN_lWrist3 DSN_lWrist4 DSN_lWrist5 DSN_lWrist6 DSN_lWrist7;		pickWalk -d down;		select -add DSN_lWrist;						parent -r -s;		delete DSN_lWrist2 DSN_lWrist3 DSN_lWrist4 DSN_lWrist5 DSN_lWrist6 DSN_lWrist7;		select DSN_lWrist;		scale -r 0.25 0.25 0.25;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		select -add DSN_MAIN;		parent;		makeIdentity -apply false -t 1 -r 1 -s 1 -n 0;		parent-w;		select DSN_lElbow DSN_lWrist;		pointConstraint;		delete DSN_lWrist_pointConstraint1;		select DSN_lWrist;		move -r -os -wd 3.5 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;						duplicate -rr -n DSN_lThumb1;		move -r -os -wd 0.45 0 0.51;		scale 0.75 0.75 0.75;		makeIdentity -apply true -t 1 -r 1 -s 1 