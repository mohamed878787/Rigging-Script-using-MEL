25;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		select -add DSN_MAIN;		parent;		makeIdentity -apply false -t 1 -r 1 -s 1 -n 0;		parent-w;		select DSN_rElbow DSN_rWrist;		pointConstraint;		delete DSN_rWrist_pointConstraint1;		select DSN_rWrist;		move -r -os -wd -3.5 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;						duplicate -rr -n DSN_rThumb1;		move -r -os -wd -0.45 0 0.51;		scale 0.75 0.75 0.75;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rThumb2;		move -r -os -wd 0 0 0.75;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rThumb3;		move -r -os -wd 0 0 0.75;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rThumbTip;		move -r -os -wd 0 0 0.75;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;				//RIGHT FINGERS		select DSN_rWrist;		duplicate -rr -n DSN_rIndex1;		move -r -os -wd -1.47 0 0.61;		scale 0.62 0.62 0.62;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rIndex2;		move -r -os -wd -0.61 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rIndex3;		move -r -os -wd -0.61 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rIndexTip;		move -r -os -wd -0.61 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;				select DSN_rWrist;		duplicate -rr -n DSN_rMiddle1;		scale 0.65 0.65 0.65;		move -r -os -wd -1.5 0 0.21;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rMiddle2;		move -r -os -wd -0.63 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rMiddle3;		move -r -os -wd -0.63 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rMiddleTip;		move -r -os -wd -0.63 0 0;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;				select DSN_rWrist;		duplicate -rr -n DSN_rRing1;		scale 0.6 0.6 0.6;		move -r -os -wd -1.5 0 -0.19;		makeIdentity -apply true -t 1 -r 1 -s 1 -n 0;		duplicate -rr -n DSN_rRing2;		move -r -os